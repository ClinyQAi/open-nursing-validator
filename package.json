{
  "name": "nhs-unified-nursing-validator",
  "version": "1.0.0",
  "description": "FHIR R4 validation API for the Open Nursing Core Implementation Guide (ONC-IG)",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "start:prod": "node dist/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "tsc --outDir dist",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lucide-react": "^0.562.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^24.10.1",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "NHS",
    "nursing",
    "fhir",
    "validation",
    "healthcare",
    "open-nursing-core"
  ],
  "author": "Lincoln Gombedza <lincolngombedza@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClinyQAi/nhs-unified-nursing-validator.git"
  },
  "homepage": "https://clinyqai.github.io/open-nursing-core-ig/"
}